                                <t t-if="state.vendor_error.error_address != null ">
                                    <p class="error-message-rack "  style="position:absolute;top:" t-esc="state.vendor_error.error_address" />
                                </t>
                            </div>
                            <div class=" material-textfield-vendor2">
                                <div class="material-textfield-vendor1">
                                    <input type="text" id="sonumber" class="input-field-vendor" name="city"  value="Coimbatore" placeholder="City " t-on-change='onchangevendordata'   required="true" />
                                    <label  class="input-label-vendor" for="sonumber">City</label>
                                </div>
                                <div class="material-textfield-vendor1">
                                    <input type="text" id="sonumber" class="input-field-vendor" name="state"  value="Tamil Nadu" placeholder="State " t-on-change='onchangevendordata'   required="true" />
                                    <label  class="input-label-vendor" for="sonumber">State</label>
                                </div>
                            </div>
                            <div class="material-textfield-vendor">
