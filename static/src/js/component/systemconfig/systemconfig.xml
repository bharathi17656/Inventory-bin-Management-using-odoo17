                                    <input class="form-check-input" type="checkbox" value="" name="emp" t-on-click="onchangeuserdata" id="flexCheckChecked"  />
                                    <label class="form-check-label" for="flexCheckChecked">
                                        Employee
                                    </label>
                                </div>
                                </t>
                            </div>
                            <div class="material-textfield-user ">
                                <input type="password" class="input-field-user"  id="sonumber" name="password" value='' t-on-change='onchangeuserdata' placeholder="" required="true" maxlength="8"  />
                                <label  class="input-label-user required1 " for="sonumber">Password</label>
                            </div>
                            <t t-if="state.user_error.error_pass != null ">
                                <p class="error-message-rack " style="position:absolute;top:230px; " t-esc="state.user_error.error_pass" />
                            </t>
                            <div class="material-textfield-user ">
                                <input type="password" class="input-field-user" id="sonumber" name="confirmpassword" value='' t-on-change='onchangeuserdata' placeholder=" "  required="true" maxlength="8"  />
                                <label  class="input-label-user required1 " for="sonumber">Confirm Password</label>
                            </div>
                            <t t-if="state.user_error.error_confirm_pass != null ">
                                <p class="error-message-rack " t-esc="state.user_error.error_confirm_pass" />
                            </t>
                        </form>
                        <div  class="div-sys-button">
                            <button class="button-inventory1"  t-on-click='submituser' style="width:90px;"  type="button">Save</button>
                            <button class="button-inventory1"  t-on-click='clearuser' style="background:red !important;width:90px;" type="clear">Clear</button>
                        </div>
                    </div>
                </t>
                <!-- vendor -->
                <t t-if="state.updatepage == 'vendor' ">
                    <div class="form-container-vendor ">
