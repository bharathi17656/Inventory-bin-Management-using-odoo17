<templates>
    <t t-name="SystemSettings">
        <div class='tab'>
            <!-- <div class="material-textfield">
                <input
      class="input-field"
      type="text"
      placeholder=""
    />
                <label class="input-label">Your Name</label>
            </div> -->
            <t t-if="!(state.itsemployee)" >
                <button class="tab-button "   t-att-class="'tab-button ' + (state.updatepage === 'user' ? 'active-tab' : '')"  t-on-click="updateuser">User</button>
            
            <button class="tab-button "   t-att-class="'tab-button ' + (state.updatepage === 'vendor' ? 'active-tab' : '')" t-on-click="updatevendor">Vendor</button>
            <button class="tab-button"  t-att-class="'tab-button ' + (state.updatepage === 'customer' ? 'active-tab' : '')"  t-on-click="updatecustomer">Customer</button>
           
                <button class="tab-button "   t-att-class="'tab-button ' + (state.updatepage === 'resetpassword' ? 'active-tab' : '')"   t-on-click="updateresetpassword" >Reset Password</button>
            </t>
        </div>
        <div t-att-class="state.popupmode ? 'twogrid1-user main-popup-hidden' : 'twogrid1-user' ">
            <div class="bin-form-user">
                <t t-if="state.updatepage == 'user' ">
                    <div class="form-container-user ">
                        <form id="dropdownForm" >
                            <div class="material-textfield-user">
                                <input type="text" id="date" name="name" value='' t-on-change='onchangeuserdata' placeholder=" " required="true"  class="input-field-user "/>
                                <label for="date" class="input-label-user required"> Name </label>
                            </div>
                            <t t-if="state.user_error.error_name != null ">
                                <p class="error-message-rack" style="position:absolute;top:90px; " t-esc="state.user_error.error_name" />
                            </t>
                            <div class="material-textfield-user">
                                <input type="text" id="sonumber" class="input-field-user" name="email" value='' t-on-change='onchangeuserdata'  placeholder=" "   required="true" />
                                <label for="vendorname" class="input-label-user required">User ID </label>
                            </div>
                           
                           <div class="select-type-user">


                            <t t-if="state.userdata.type != 'emp'" >
                            <div class="form-check">
                                    <input class="form-check-input" type="checkbox" name="admin" t-on-click="onchangeuserdata" value="" id="flexCheckDefault" />
                                    <label class="form-check-label" for="flexCheckDefault">
                                        Admin
                                    </label>
                                    </div>

                            </t>
                             <t t-if="state.userdata.type != 'admin'" >
